[
  {
    "name": "testapp",
    "image": "${app_image}",
    "cpu": ${fargate_cpu},
    "memory": ${fargate_memory},
    "networkMode": "awsvpc",
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/testapp",
          "awslogs-region": "${aws_region}",
          "awslogs-stream-prefix": "ecs"
        }
    },
    "portMappings": [
      {
        "containerPort": ${app_port},
        "hostPort": ${app_port}
      }
    ],
    "environment": [
      {
        "name": "AccessKeyId",
        "value": ${key_id}
      },
      {
        "name": "secretAccessKey",
        "value": ${secret_key}
      },
      {
        "name": "BROKER_URL",
        "value": ${url_broker}
      }
    ]
  }
]
